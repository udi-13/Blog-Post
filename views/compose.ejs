<%- include("partials/header.ejs") -%>
<h1>Compose</h1>
<form class="form-group" action="/compose" method="post">
  <p>
    <b>Title</b>
  </p>
  <input class="form-control" type="text" name="postTitle" placeholder="" autocomplete="off">
  <p>
    <b>Post</b>
  </p>
  <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
  <button class="btn btn-primary" type="submit" name="button" style="margin-top:4%">Publish</button>
</form>
<%- include("partials/footer.ejs") -%>
